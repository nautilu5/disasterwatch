<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<android.support.design.widget.AppBarLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:theme="@style/AppTheme.AppBarOverlay">
	</android.support.design.widget.AppBarLayout>
	<fragment
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/map"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		class="com.google.android.gms.maps.MapFragment" />
	<Button
		android:id="@+id/button"
		android:layout_width="35dp"
		android:layout_height="35dp"
		android:translationY="0dp"
		android:translationX="0dp"
		android:padding="10dp"
		android:background="@android:drawable/ic_menu_add"/>
	<TextView
		android:id="@+id/pins"
		android:text = "Place Pins"
		android:layout_width = "80dp"
		android:layout_height="35dp"
		android:translationX="35dp"
		android:translationY="7.5dp"
	/>
	<Button
		android:id="@+id/button_2"
		android:layout_width="35dp"
		android:layout_height="35dp"
		android:translationY="30dp"
		android:translationX="0dp"
		android:padding="10dp"
		android:background="@android:drawable/ic_menu_add"/>
	<TextView
		android:id="@+id/water"
		android:text = "Water Hazard"
		android:layout_width = "90dp"
		android:layout_height="35dp"
		android:translationX="35dp"
		android:translationY="37.5dp"
	/>
	<Button
		android:id="@+id/button_3"
		android:layout_width="35dp"
		android:layout_height="35dp"
		android:translationY="60dp"
		android:translationX="0dp"
		android:padding="10dp"
		android:background="@android:drawable/ic_menu_add"/>
	<TextView
		android:id="@+id/fire"
		android:text = "Fire Hazard"
		android:layout_width = "80dp"
		android:layout_height="35dp"
		android:translationX="35dp"
		android:translationY="67.5dp"
	/>
	<Button
		android:id="@+id/button_4"
		android:layout_width="35dp"
		android:layout_height="35dp"
		android:translationY="90dp"
		android:translationX="0dp"
		android:padding="10dp"
		android:background="@android:drawable/ic_menu_add"/>
	<TextView
		android:id="@+id/debris"
		android:text = "Debris Hazard"
		android:layout_width = "90dp"
		android:layout_height="35dp"
		android:translationX="35dp"
		android:translationY="97.5dp"
	/>
	<Button
		android:id="@+id/button_5"
		android:layout_width="35dp"
		android:layout_height="35dp"
		android:translationY="120dp"
		android:translationX="0dp"
		android:padding="10dp"
		android:background="@android:drawable/ic_menu_add"/>
	<TextView
		android:id="@+id/road"
		android:text = "Blocked Road"
		android:layout_width = "90dp"
		android:layout_height="35dp"
		android:translationX="35dp"
		android:translationY="127.5dp"
	/>
	<Button
		android:id="@+id/safehouse"
		android:layout_width="55dp"
		android:layout_height="55dp"
		android:layout_gravity="bottom"
		android:padding="10dp"
		android:background="@android:drawable/ic_menu_add"/>
	<TextView
		android:id="@+id/road"
		android:text = "Register a shelter"
		android:layout_width = "110dp"
		android:layout_height="35dp"
		android:translationX="55dp"
		android:layout_gravity="bottom"
	/>
	<TextView
		android:id="@+id/supplies_text"
		android:text = "Supplies Needed"
		android:layout_width = "110dp"
		android:layout_height="35dp"
		android:translationY="-45dp"
		android:layout_gravity="center"
	/>
	<EditText 
		android:id="@+id/supplies"
		android:layout_width = "210dp"
		android:layout_height="55dp"
		android:layout_gravity="center"
		android:translationY="-35dp"
	/>
	
</android.support.design.widget.CoordinatorLayout>